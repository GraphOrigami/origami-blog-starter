(siteInfo) => `
    <meta name="description" content="${ siteInfo/description }">
    <link rel="canonical" href="${ siteInfo/customDomain }">
    <meta property="og:title" content="${ siteInfo/title }">
    <meta property="og:type" content="article">
    <meta property="og:url" content="${ siteInfo/customDomain }">
    <meta property="og:description" content="${ siteInfo/description }">
    <meta property="og:image" content="${ siteInfo/customDomain }/${ siteInfo/image }">
`
